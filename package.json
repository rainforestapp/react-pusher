    "enzyme": "^2.3.0",
    "eslint-config-rainforest": "^0.1.2",
    "eslint-plugin-babel": "^3.2.0",
    "eslint-plugin-flow-vars": "^0.4.0",
    "eslint-plugin-import": "^1.8.1",
